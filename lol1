interface primates {
}

class human implements primates {

int height;
string[] quality;
int counter;

void setZero() { counter = 0; quality = NewArray(10, string); }

void qualityAssign(string lol) {
if (counter < 10) {
   quality[counter] = lol;
   counter = counter + 1;
}
else Print("error array of qualities is full");
}

string getQuality() { return quality; }

int getCounter() {return counter; }

int[] lol(human yolo) { int[] a; a = NewArray(300 *7, int); return a;}

}

int lol1(human hey) {
bool joke;
int lol;
int lol4;
human sam;
human maggie;

sam.setZero();
sam.qualityAssign("big butt");
sam.qualityAssign("tall");
sam.qualityAssign("not funny");
sam.qualityAssign("nice hair");

maggie.setZero();
maggie.qualityAssign("pretty");
maggie.qualityAssign("nice butt");
maggie.qualityAssign(" eyes");
maggie.qualityAssign("");

getInfo(sam); }

void getInfo(human H){
 int i;
 for (i = 0; i < H.getCounter(); i = i + 1)
   Print(H.getQuality(), i);
}

